{
    "functions": {
      "api/*.php": {
        "runtime": "@vercel/php@0.7.0"
      },
      "api/*.js": {
        "runtime": "nodejs@18.17.0"
      }
    },
    "routes": [
      { "src": "^/$", "dest": "/api/dashboard.php" },
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/api/$1" }
    ],
    "build": {
      "env": {
        "NOW_PHP_DEBUG": "1"
      }
    }
  }